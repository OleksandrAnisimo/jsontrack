{
  "trailingComma": "es5",
  "singleQuote": false,
  "semi": true,
  "printWidth": 100,
  "arrowParens": "avoid",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "styled",
    "<THIRD_PARTY_MODULES>",
    "^src/(.*)$",
    "^utils/(.*)$",
    "^assets/(.*)$",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
